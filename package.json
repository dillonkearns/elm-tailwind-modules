{
  "name": "elm-tailwind-origami",
  "version": "0.0.1",
  "author": "Philipp Kr√ºger",
  "description": "Generate Elm code for Tailwind Utilities and Components that purges using Elm's --optimize!",
  "repository": {
    "type": "git",
    "url": "https://github.com/matheus23/elm-tailwind-origami"
  },
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "test": "yarn test:clean && yarn test:elm-compile && jest",
    "test:clean": "rm -rf test/src && rm -rf test/gen && rm -rf test/dist",
    "test:elm-compile": "cd test && node run-postcss.js && elm make Main.elm",
    "test:node-only": "cd test && node --trace-warnings --unhandled-rejections=strict run-postcss.js"
  },
  "keywords": [
    "postcss",
    "postcss-plugin",
    "elm",
    "tailwind",
    "tailwindcss"
  ],
  "license": "MIT",
  "peerDependencies": {
    "tailwindcss": "^1.6.2"
  },
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "elm": "^0.19.1-3",
    "elm-format": "^0.8.3",
    "jest": "^26.2.2",
    "postcss-cli": "^7.1.1",
    "tailwindcss": "^1.6.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
