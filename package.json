{
  "name": "elm-tailwind-origami",
  "version": "0.0.1",
  "author": "Philipp Kr√ºger",
  "description": "Generate Elm code for Tailwind Utilities and Components that purges using Elm's --optimize!",
  "repository": {
    "type": "git",
    "url": "https://github.com/matheus23/elm-tailwind-origami"
  },
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "test": "ava",
    "test-all": "yarn test:clean && yarn test:elm-compile && yarn test",
    "test:clean": "rm -r test-example/src/Tailwind || true",
    "test:elm-compile": "node --trace-warnings --unhandled-rejections=strict test-example/test-run.js && cd test-example && elm make src/Main.elm --output=/dev/null",
    "test:node-only": "node --trace-warnings --unhandled-rejections=strict test-example/test-run.js"
  },
  "ava": {
    "files": [ "test/**/*" ]
  },
  "keywords": [
    "postcss",
    "postcss-plugin",
    "elm",
    "tailwind",
    "tailwindcss"
  ],
  "license": "MIT",
  "dependencies": {
    "@tailwindcss/ui": "^0.7.2",
    "autoprefixer": "^10.2.1",
    "css-what": "^4.0.0",
    "deep-equal": "^2.0.3",
    "elm": "^0.19.1-3",
    "elm-format": "^0.8.3",
    "fs-extra": "^9.0.1",
    "jest": "^26.4.0",
    "lodash": "^4.17.20",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "postcss": "^8.2.4"
  },
  "peerDependencies": {
    "postcss": "^8.2.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
